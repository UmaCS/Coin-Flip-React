esktop/treeHouse/Projects/coin_flip_react/src/CoinContainer.js",["30"],"import { render } from '@testing-library/react';\nimport React, {Component} from 'react';\nimport Coin from './Coin';\nimport './Coin.css'\nimport {choice} from './helpers.js';\n\nclass CoinContainer extends Component {\n    static defaultProps = {\n        coins: [\n            {side:'heads', imgSrc: \"https://coin-brothers.com/photos/Uzbekistan_Tiyin_1/1994_24.12.2016_01.38_02.jpg\"},\n            {side:'tails', imgSrc: \"https://coin-brothers.com/photos/Uzbekistan_Tiyin_3/1994_24.12.2016_03.16.jpg\"}\n        ]\n    }\n    constructor(props){\n        super(props);\n        this.state = {\n            currCoin: null,\n            nFlips: 0,\n            nHeads: 0,\n            nTails: 0\n        }\n        this.handleClick = this.handleClick.bind(this);\n    }\n    flipCoin(){\n        const newCoin = choice(this.props.coins);\n        return this.setState(st => {\n            return {\n                currCoin: newCoin, \n                nFlips: st.nFlips + 1,\n                nHeads: st.nHeads + (newCoin.side === 'heads' ? 1 : 0),\n                nTails: st.nTails + (newCoin.side === 'tails' ? 1 : 0)\n            };\n        });\n        \n    }\n\n    handleClick(e){\n        this.flipCoin()\n    }\n    render(){\n        return (\n            <div className=\"Head\">\n                <h1>Let's flip a coin!</h1>\n                {this.state.currCoin && <Coin info={this.state.currCoin} />}\n                <button onClick={this.handleClick}>Flip me</button>\n                <h3>{`Out of ${this.state.nFlips} flips, there have been ${this.state.nHeads} heads and ${this.state.nTails} tails.`}</h3>\n                <br/>\n                {this.state.currCoin && <p>This coin is called \"1 tiyn\" in my homecountry(Uzbekistan), it is like a US penny</p>}\n            </div>\n        );\n    }\n}\n\nexport default CoinContainer;","/Users/mbp-2017/Desktop/treeHouse/Projects/coin_flip_react/src/help